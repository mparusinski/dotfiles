function config {
    /usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME $@;
}

alias gp="git push"
alias gf="git fetch"
alias gc="git commit"

function saq {
    eval `ssh-agent`
    ssh-add
}

alias ls="nnn -de"
if [ -f /usr/share/nnn/quitcd/quitcd.bash_zsh ]; then
    source /usr/share/nnn/quitcd/quitcd.bash_zsh
fi
